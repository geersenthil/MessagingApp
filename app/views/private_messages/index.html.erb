<h1>Private Messages</h1>
<table>
  <thead>
    <tr>
      <th>Directmessage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @private_messages_sent.each do |private_message| %>
      <tr>
        <td><%= private_message.directmessage %></td>
        <td><%= link_to 'Show', private_message %></td>
        <td><%= link_to 'Edit', edit_private_message_path(private_message) %></td>
        <td><%= link_to 'Destroy', private_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>


<%= link_to 'New Private Message', new_private_message_path %>
